<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="top.xiesen.video.mapper.SubjectMapper" >

     <select id="selectAllSubject" resultType="subject">
        select * from subject
    </select>

    <select id="selectSubjectById" resultType="subject" parameterType="String">
        select * from subject WHERE id=#{0}
    </select>
</mapper>